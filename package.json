    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.4",
    "nodemon": "^2.0.19",
    "react-flip-toolkit": "^7.0.14",
    "react-icons": "^4.4.0",
